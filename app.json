{
  "expo": {
    "name": "ReactNativeProject",
    "slug": "ReactNativeProject",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
         "NSPhotoLibraryUsageDescription": "Access to your photos"
      },
      "config": {
         "googleMaps": {"apiKey": "AIzaSyAoEXZsOxIuBoEGcnHFmnEY8na8-zaW-gw"}
      },
      "supportsTablet": true
    },
    "android": {
      "permissions": ["CAMERA_ROLL", "READ_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
